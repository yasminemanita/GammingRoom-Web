<!-- Slider Revolution -->
<div class="rev_slider_wrapper rs-youplay rs-fullscreen">
    <div class="rev_slider" data-version="5.4.6.4">
        <ul>
            <!-- SLIDE NR. 1  -->
            <li data-index="rs-1" data-thumb="assets/images/demo-dark.jpg"  data-saveperformance="on" data-transition="random-static" data-slotamount="7" data-masterspeed="700">
                <!-- MAIN IMAGE -->
                <img src="assets/images/demo-dark.jpg"  alt="" data-lazyload="assets/images/demo-dark.jpg" data-bgposition="center top" data-bgfit="cover" data-bgrepeat="no-repeat" class="rev-slidebg">
                <!-- LAYERS -->

                <!-- LAYER NR. 1 -->
                <div class="tp-caption customin customout"
                     data-x="left"
                     data-hoffset="60"

                     data-y="center"
                     data-voffset="-80"

                     data-customin="x:200;scaleX:0.5;scaleY:0.5;"
                     data-customout="x:0;scaleX:1;scaleY:1;"

                     data-frames='[{"from":"opacity:0;y:-10","speed":1000,"to":"o:1;","delay":500,"ease":"Power2.easeInOut"},{"delay":"wait","speed":1000,"to":"opacity:0;","ease":"nothing"}]'
                >

                </div>

                <!-- LAYER NR. 2 -->
                <div class="tp-caption customin customout"
                     data-x="left"
                     data-hoffset="60"

                     data-y="center"
                     data-voffset="80"

                     data-frames='[{"from":"opacity:0;y:10","speed":1000,"to":"o:1;","delay":500,"ease":"Power2.easeInOut"},{"delay":"wait","speed":1000,"to":"opacity:0;","ease":"nothing"}]'
                >

                </div>
            </li>

            <!-- SLIDE NR. 2  -->
            <li data-index="rs-2" data-thumb="assets/images/demo-dark.jpg" data-saveperformance="on" data-transition="random-static" data-slotamount="7" data-masterspeed="700">
                <!-- MAIN IMAGE -->
                <img src="assets/images/demo-dark.jpg"  alt="" data-lazyload="assets/images/demo-dark.jpg" data-bgposition="center top" data-bgfit="cover" data-bgrepeat="no-repeat" class="rev-slidebg">
                <!-- LAYERS -->

                <!-- LAYER NR. 1 -->
                <div class="tp-caption customin customout"
                     data-x="left"
                     data-hoffset="60"

                     data-y="center"
                     data-voffset="-65"

                     data-frames='[{"from":"opacity:0;y:-10","speed":1000,"to":"o:1;","delay":500,"ease":"Power2.easeInOut"},{"delay":"wait","speed":1000,"to":"opacity:0;","ease":"nothing"}]'
                >

                </div>

                <!-- LAYER NR. 2 -->
